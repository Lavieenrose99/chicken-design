    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "@types/prismjs": "^1.16.2",
    "@types/react-copy-to-clipboard": "^5.0.0",
    "cpr": "^3.0.1",
    "docz": "^2.3.1",
    "gatsby-plugin-less": "^4.5.0",
    "less": "^4.1.0",
    "prismjs": "^1.23.0",
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.2",
